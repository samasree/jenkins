Jenkinsfile (Declarative Pipeline)
pipeline { 
  agent any
  stages {
    stage('Build') {
      steps {
        sh 'echo "Build"'
      }
    }
    stage('Test') {
      steps {
        sh 'echo "Testing"'
      }
    }
    stage('Staging') {
      steps {
        sh 'echo "Staging"'
      }
    }
    stage('Production') {
      steps {
        sh 'echo "production"'
      }
    }
  }
}
